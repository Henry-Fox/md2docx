{
  "document": {
    "pageSize": "A4",
    "pageOrientation": "portrait",
    "margins": {
      "top": 2099,
      "right": 1474,
      "bottom": 1984,
      "left": 1587
    },
    "grid": {
      "charPerLine": 28,
      "linePerPage": 22
    }
  },
  "heading": {
    "font": {
      "name": "方正小标宋简体"
    },
    "color": "000000",
    "styles": {
      "h1": {
        "font": {
          "name": "方正小标宋简体",
          "size": 22,
          "bold": false
        },
        "paragraph": {
          "alignment": "center",
          "spacing": {
            "before": 240,
            "after": 120
          },
          "indent": {
            "left": 0,
            "firstLine": 0
          }
        },
        "numbering": {
          "format": "decimal",
          "level": 0,
          "template": "第 %1 章",
          "usePrefix": false
        }
      },
      "h2": {
        "font": {
          "name": "黑体",
          "size": 16,
          "bold": true
        },
        "paragraph": {
          "alignment": "left",
          "spacing": {
            "before": 160,
            "after": 80
          },
          "indent": {
            "left": 0,
            "firstLine": 0
          }
        },
        "numbering": {
          "format": "decimal",
          "level": 1,
          "parentLevel": 0,
          "template": "%1.%2",
          "usePrefix": true,
          "prefix": "一、"
        }
      },
      "h3": {
        "font": {
          "name": "楷体",
          "size": 16,
          "bold": false
        },
        "paragraph": {
          "alignment": "left",
          "spacing": {
            "before": 120,
            "after": 60
          },
          "indent": {
            "left": 0,
            "firstLine": 0
          }
        },
        "numbering": {
          "format": "decimal",
          "level": 2,
          "parentLevel": 1,
          "template": "%1.%2.%3",
          "usePrefix": true,
          "prefix": "(一)"
        }
      },
      "h4": {
        "font": {
          "name": "仿宋_GB2312",
          "size": 16,
          "bold": false
        },
        "paragraph": {
          "alignment": "left",
          "spacing": {
            "before": 120,
            "after": 60
          },
          "indent": {
            "left": 800,
            "firstLine": 0
          }
        },
        "numbering": {
          "format": "decimal",
          "level": 3,
          "parentLevel": 2,
          "template": "%1.%2.%3.%4",
          "usePrefix": true,
          "prefix": "1."
        }
      },
      "h5": {
        "font": {
          "name": "仿宋_GB2312",
          "size": 16,
          "bold": false
        },
        "paragraph": {
          "alignment": "left",
          "spacing": {
            "before": 120,
            "after": 60
          },
          "indent": {
            "left": 800,
            "firstLine": 0
          }
        },
        "numbering": {
          "format": "decimal",
          "level": 4,
          "parentLevel": 3,
          "template": "%1.%2.%3.%4.%5",
          "usePrefix": true,
          "prefix": "(1)"
        }
      },
      "h6": {
        "font": {
          "name": "仿宋_GB2312",
          "size": 14,
          "bold": false
        },
        "paragraph": {
          "alignment": "left",
          "spacing": {
            "before": 120,
            "after": 60
          },
          "indent": {
            "left": 0,
            "firstLine": 0
          }
        },
        "numbering": {
          "format": "lowerAlpha",
          "level": 5,
          "parentLevel": 4,
          "template": "%1.%2.%3.%4.%5.%6",
          "usePrefix": true,
          "prefix": "a."
        }
      }
    }
  },
  "paragraph": {
    "font": "仿宋_GB2312",
    "size": 16,
    "color": "000000",
    "firstLineIndent": 800,
    "alignment": "justified",
    "lineSpacingRule": "auto",
    "lineSpacing": 1.5,
    "spacing": 0
  },
  "code": {
    "font": "等线",
    "size": 14,
    "color": "000000",
    "backgroundColor": "F5F5F5"
  },
  "table": {
    "borderColor": "000000",
    "borderWidth": 1,
    "headerBackground": "E6E6E6",
    "headerFont": "仿宋_GB2312",
    "fontSize": 16,
    "alignment": "center"
  },
  "list": {
    "unordered": {
      "font": "仿宋_GB2312",
      "size": 16,
      "bulletChars": ["●", "○", "■"],
      "indentLevel": 800
    },
    "ordered": {
      "font": "仿宋_GB2312",
      "size": 16,
      "numberFormats": ["%1、", "（%2）", "%3."],
      "indentLevel": 800
    },
    "task": {
      "font": "仿宋_GB2312",
      "size": 16,
      "color": "#000000",
      "completedChar": "☑",
      "uncompletedChar": "☐"
    }
  },
  "blockquote": {
    "font": "仿宋_GB2312",
    "size": 16,
    "color": "000000",
    "borderColor": "000000",
    "leftIndent": 600,
    "firstLineIndent": 800
  },
  "link": {
    "color": "#0066CC",
    "underline": true
  },
  "image": {
    "maxWidth": 6000,
    "alignment": "center"
  },
  "horizontalRule": {
    "color": "#000000",
    "thickness": 1,
    "spacing": 240
  },
  "footnote": {
    "font": "仿宋_GB2312",
    "size": 14,
    "color": "000000"
  },
  "textStyles": {
    "bold": {
      "bold": true
    },
    "italic": {
      "italic": true
    },
    "underline": {
      "underline": true
    },
    "strikethrough": {
      "strike": true
    }
  }
}
